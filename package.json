    "jsdom": "^11.5.1",
    "mocha": "^4.0.1",
    "nyc": "^11.4.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
